{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "memory": 1024,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".:/var/task",
    "SETUPTOOLS_USE_DISTUTILS": "stdlib",
    "PIP_DISABLE_PIP_VERSION_CHECK": "1",
    "SETUPTOOLS_SCM_PRETEND_VERSION": "1.0.0"
  }
}