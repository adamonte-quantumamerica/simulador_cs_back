{
  "version": 2,
  "builds": [
    {
      "src": "wesolar/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "wesolar/wsgi.py"
    }
  ],
  "env": {
    "DEVELOPMENT": "False",
    "SECRET_KEY": "@secret-key",
    "DATABASE_URL": "@database-url",
    "DEBUG": "False"
  }
}
